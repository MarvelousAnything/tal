{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "cortex-debug",
      "cwd": "${workspaceFolder}",
      "configFiles": [
        "${workspaceFolder}/bin/openocd/devboard.cfg",
      ],
      "device": "STM32H745ZIT3",
      "interface":"swd",
      "name": "dev debug cm7",
      "request": "launch",
      "runToEntryPoint": "",
      "serialNumber": "",
      "servertype": "openocd",
      "svdFile": "${workspaceFolder}/bin/svd/STM32H745_CM7.svd",
      "swoConfig": {
        "enabled": true,
        "cpuFrequency": 480000000,
        "swoFrequency": 2000000,
        "source": "probe",
        "decoders": [
          {
            "type": "console",
            "name": "ITM",
            "port": 0
          }
        ]
      },
      "armToolchainPath": "${workspaceFolder}/build/toolchain_cache/toolchain/arm-gnu-toolchain/bin",
      "gdbPath": "${workspaceFolder}/build/toolchain_cache/toolchain/arm-gnu-toolchain/bin/arm-none-eabi-gdb",
      "executable": "${workspaceFolder}/build/titan.elf",
      "numberOfProcessors": 2,
      "targetProcessor": 0
    }
  ]
}